{
  "author": "Florian Keller <github@floriankeller.de>",
  "dependencies": {},
  "description": "Import CSV to splitwise",
  "devDependencies": {
    "@ffflorian/eslint-config": "0.5.1",
    "@ffflorian/prettier-config": "0.0.6",
    "@ffflorian/typescript-config": "1.0.1",
    "@types/node": "~12",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jasmine": "4.1.1",
    "eslint-plugin-jsdoc": "28.0.0",
    "eslint-plugin-no-unsanitized": "3.1.2",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-simple-import-sort": "5.0.3",
    "eslint-plugin-sort-keys-fix": "1.1.1",
    "eslint-plugin-typescript-sort-keys": "1.2.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "rimraf": "3.0.2",
    "ts-node": "8.10.2",
    "typescript": "3.9.5"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "server",
    "client",
    "rest",
    "split"
  ],
  "license": "GPL-3.0",
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "splitwise-import",
  "prettier": "@ffflorian/prettier-config",
  "repository": "https://github.com/ffflorian/splitwise-import.git",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "dist": "yarn clean && yarn build",
    "fix": "yarn fix:other && yarn fix:ts",
    "fix:other": "yarn prettier --write",
    "fix:ts": "yarn lint:ts --fix",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "eslint --ignore-path .gitignore --ext .ts src/",
    "prettier": "prettier \"*.{json,md}\"",
    "test": "exit 0"
  },
  "version": "0.0.1"
}
